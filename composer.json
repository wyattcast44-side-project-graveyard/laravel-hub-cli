{
    "name": "laravel-hub-cli/installer",
    "description": "A tool to scaffold new Laravel applications",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Wyatt Castaneda",
            "email": "wyatt.castaneda@gmail.com"
        }
    ],
    "require": {
        "illuminate/console": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/events": "^8.0",
        "nunomaduro/collision": "^5.0",
        "symfony/filesystem": "^5.0",
        "symfony/process": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelHub\\Installer\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}